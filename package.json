    "@types/webpack-bundle-analyzer": "^2.13.1",
    "@types/webpack-dev-server": "^3.1.2",
    "@typescript-eslint/parser": "^1.4.2",
    "classnames": "^2.2.6",
    "eslint-plugin-prettier": "^3.0.1",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
